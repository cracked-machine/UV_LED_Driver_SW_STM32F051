#include "EventManager.h"


UVBOX_SystemStateTypedef eTheSystemState = UVBOX_LIDOPEN;

void EM_ProcessEvent(UVBOX_SystemEventsTypedef newEvent)
{
	switch(eTheSystemState)
	{

		case UVBOX_LIDOPEN:
			break;

		case UVBOX_LIDCLOSED:
			break;
	}
}

